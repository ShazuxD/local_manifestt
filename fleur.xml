<manifest>
    <!-- Remotes -->
    <remote name="6781-dev" fetch="https://github.com/xiaomi-mt6781-devs"
        revision="lineage-20" />
    <remote name="lineageos" fetch="https://github.com/LineageOS"
        revision="lineage-20" />
 <remote name="shazuxd" fetch="https://github.com/ShazuxD" />
    <!-- Device / Kernel prebuilts -->
    <project path="device/xiaomi/fleur" name="device_xiaomi_fleur"
        remote="shazuxd" revsion="oss" />
    <project path="device/xiaomi/fleur-kernel"
        name="device_xiaomi_fleur-kernel"
        remote="shazuxd" revision="thirteen" />


    <!-- Hardware -->
    <project path="hardware/xiaomi" name="android_hardware_xiaomi"
        remote="lineageos" />

    <project path="hardware/mediatek" name="android_hardware_mediatek" remote="6781-dev" />

    <!-- Proprietary vendor -->
    <project path="vendor/xiaomi/fleur" name="vendor_xiaomi_fleur"
        remote="shazuxd" revision="stock" />

</manifest>
